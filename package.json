{
  "name": "marvel-fight-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db": "docker-compose up -d",
    "model": "sequelize model:create --name $name --attributes $attributes",
    "migration": "sequelize migration:create --name $name",
    "migrate:all": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "sequelize seed:create --name $name",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "astro": "^3.0.10",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "flowbite": "^1.8.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.33.0",
    "tailwindcss": "^3.3.3",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "daisyui": "^3.7.3",
    "tailwind-datepicker-react": "^1.4.3"
  }
}
